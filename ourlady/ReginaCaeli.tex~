\newHymn
\FirstLine{Regina coeli, laetare, alleluia}
%\JustHymnNum

\HymnTitleit{Marian antiphon for Paschaltide}

\gresetfirstlineaboveinitial{6}{6}
\includescore{gabc/reginacaeli.tex}
\gresetfirstlineaboveinitial{}{}


%\begin{verse}[\versewidth]

%\FirstVerse{R}{egina} coeli, laetáre, allelúia!\\
%Quia, quem meruísti portáre, allelúia!\\
%Resurréxit, sicut dixit, allelúia!\\
%Ora pro nobis Deum, allelúia!

%\end{verse}

\Hnote{O Queen of heaven, rejoice, alleluia.}
\Hnote{For He whom thou didst merit to bear, alleluia.}
\Hnote{Is risen as He said, alleluia.}
\Hnote{Pray for us to God, alleluia.}
